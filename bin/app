#!/usr/bin/php -q
<?php
    require ("App/Core/Autoload/Autoload.php");
    
    $autoload = new Autoload();
    $autoload->loadClass();
    
    $loran = new App\Core\Application\AppCli();
    $loran->sendCommand(@htmlentities($argv[1]), @htmlentities($argv[2]), @htmlentities($argv[3]), @htmlentities($argv[4]));
?>


